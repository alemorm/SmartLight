{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Key Handling Restore",
        "type": "shell",
        "command": "${workspaceFolder}/keyHandler.sh -b",
        "group": {
            "kind": "test",
            "isDefault": true
        }
      },
      {
        "label": "Key Handler Anonymize",
        "type": "shell",
        "command": "${workspaceFolder}/keyHandler.sh -a"
      },
      {
          "label": "Git Pull",
          "type": "shell",
          "command": "git pull"
      },
      {
          "label": "Git Add All",
          "type": "shell",
          "command": "git add -A"
      },
      {
          "label": "Git Commit All",
          "type": "shell",
          "command": "git commit -a"
      },
      {
          "label": "Git Sync",
          "type": "shell",
          "command": "git push",
          "dependsOrder": "sequence",
          "dependsOn": [
              "Git Pull",
              "Key Handler Anonymize",
              "Git Add All",
              "Git Commit All"
          ]
      }
    ]
  }